@model Register
@{
ViewData["Title"] = "Completed";
}
<div class="bg-primary text-white m-1 p-1">
    <h2>Register is completed</h2>
</div>

<table class="table  table-stripped m-1 p-1">
    <tr>
        <td>UserName</td>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <td>Email</td>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <td>BirthDate</td>
        <td>@Model.BirthDate</td>
    </tr>
</table>